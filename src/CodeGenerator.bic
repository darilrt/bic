import "iostream";
import "tuple";
import "AST.bic";

class CodeGenerator {
    pub Generate(ast : ASTNode*) -> std::tuple<std::string, std::string> {
        std::cout << "Generating code..." << std::endl;

        mut code : std::string = "";
        mut header : std::string = "";

        ret std::make_tuple(code, header);
    }
}